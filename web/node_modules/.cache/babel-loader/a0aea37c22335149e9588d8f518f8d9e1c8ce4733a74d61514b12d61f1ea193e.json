{"ast":null,"code":"import axios from 'axios';\nimport echarts from 'echarts';\nexport default {\n  name: 'SucceedGoodsTop',\n  mounted() {\n    axios.get('http://localhost:5000/SucceedGoodsTop').then(response => {\n      const data = response.data;\n      this.renderChart(data);\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n  methods: {\n    renderChart(data) {\n      const chartDom = document.getElementById('SucceedGoodsTop');\n      const chart = echarts.init(chartDom);\n      const option = {\n        title: {\n          text: 'Item Count'\n        },\n        tooltip: {},\n        xAxis: {\n          type: 'value',\n          name: 'Count'\n        },\n        yAxis: {\n          type: 'category',\n          data: data.map(item => item.item_name),\n          name: 'Item Name'\n        },\n        series: [{\n          name: 'Count',\n          type: 'bar',\n          data: data.map(item => item.count),\n          label: {\n            show: true,\n            position: 'left'\n          }\n        }]\n      };\n      chart.setOption(option);\n    }\n  }\n};","map":{"version":3,"names":["axios","echarts","name","mounted","get","then","response","data","renderChart","catch","error","console","log","methods","chartDom","document","getElementById","chart","init","option","title","text","tooltip","xAxis","type","yAxis","map","item","item_name","series","count","label","show","position","setOption"],"sources":["/Users/raopend/IdeaProjects/spark-bilibili-analysis/web/src/components/SucceedGoodsTop.vue"],"sourcesContent":["<template>\n    <div id=\"SucceedGoodsTop\" style=\"width:100%;height:500px;\"></div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport echarts from 'echarts';\n\nexport default {\n    name: 'SucceedGoodsTop',\n    mounted() {\n        axios.get('http://localhost:5000/SucceedGoodsTop')\n            .then(response => {\n                const data = response.data;\n                this.renderChart(data)\n            })\n            .catch(error => {\n                console.log(error);\n            });\n    },\n    methods: {\n        renderChart(data) {\n            const chartDom = document.getElementById('SucceedGoodsTop');\n            const chart = echarts.init(chartDom);\n\n            const option = {\n                title: {\n                    text: 'Item Count'\n                },\n                tooltip: {},\n                xAxis: {\n                    type: 'value',\n                    name: 'Count'\n                },\n                yAxis: {\n                    type: 'category',\n                    data: data.map(item => item.item_name),\n                    name: 'Item Name'\n                },\n                series: [{\n                    name: 'Count',\n                    type: 'bar',\n                    data: data.map(item => item.count),\n                    label: {\n                        show: true,\n                        position: 'left'\n                    }\n                }]\n            };\n\n            chart.setOption(option);\n        }\n    }\n}\n</script>"],"mappings":"AAKA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,SAAS;AAE7B,eAAe;EACXC,IAAI,EAAE,iBAAiB;EACvBC,OAAOA,CAAA,EAAG;IACNH,KAAK,CAACI,GAAG,CAAC,uCAAuC,EAC5CC,IAAI,CAACC,QAAO,IAAK;MACd,MAAMC,IAAG,GAAID,QAAQ,CAACC,IAAI;MAC1B,IAAI,CAACC,WAAW,CAACD,IAAI;IACzB,CAAC,EACAE,KAAK,CAACC,KAAI,IAAK;MACZC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;IACtB,CAAC,CAAC;EACV,CAAC;EACDG,OAAO,EAAE;IACLL,WAAWA,CAACD,IAAI,EAAE;MACd,MAAMO,QAAO,GAAIC,QAAQ,CAACC,cAAc,CAAC,iBAAiB,CAAC;MAC3D,MAAMC,KAAI,GAAIhB,OAAO,CAACiB,IAAI,CAACJ,QAAQ,CAAC;MAEpC,MAAMK,MAAK,GAAI;QACXC,KAAK,EAAE;UACHC,IAAI,EAAE;QACV,CAAC;QACDC,OAAO,EAAE,CAAC,CAAC;QACXC,KAAK,EAAE;UACHC,IAAI,EAAE,OAAO;UACbtB,IAAI,EAAE;QACV,CAAC;QACDuB,KAAK,EAAE;UACHD,IAAI,EAAE,UAAU;UAChBjB,IAAI,EAAEA,IAAI,CAACmB,GAAG,CAACC,IAAG,IAAKA,IAAI,CAACC,SAAS,CAAC;UACtC1B,IAAI,EAAE;QACV,CAAC;QACD2B,MAAM,EAAE,CAAC;UACL3B,IAAI,EAAE,OAAO;UACbsB,IAAI,EAAE,KAAK;UACXjB,IAAI,EAAEA