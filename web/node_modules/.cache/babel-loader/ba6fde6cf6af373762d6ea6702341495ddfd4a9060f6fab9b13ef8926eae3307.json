is: {\n                    type: 'category',\n                    data: data.map(item => item.item_name),\n                    name: 'Item Name'\n                },\n                series: [{\n                    name: 'Count',\n                    type: 'bar',\n                    data: data.map(item => item.count),\n                    label: {\n                        show: true,\n                        position: 'left'\n                    }\n                }]\n            };\n\n            chart.setOption(option);\n        }\n    }\n}\n</script>"],"mappings":"AAKA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,SAAS;AAE7B,eAAe;EACXC,IAAI,EAAE,iBAAiB;EACvBC,OAAOA,CAAA,EAAG;IACNH,KAAK,CAACI,GAAG,CAAC,uCAAuC,EAC5CC,IAAI,CAACC,QAAO,IAAK;MACd,MAAMC,IAAG,GAAID,QAAQ,CAACC,IAAI;MAC1B,IAAI,CAACC,WAAW,CAACD,IAAI;IACzB,CAAC,EACAE,KAAK,CAACC,KAAI,IAAK;MACZC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;IACtB,CAAC,CAAC;EACV,CAAC;EACDG,OAAO,EAAE;IACLL,WAAWA,CAACD,IAAI,EAAE;MACd,MAAMO,QAAO,GAAIC,QAAQ,CAACC,cAAc,CAAC,iBAAiB,CAAC;MAC3D,MAAMC,KAAI,GAAIhB,OAAO,CAACiB,IAAI,CAACJ,QAAQ,CAAC;MAEpC,MAAMK,MAAK,GAAI;QACXC,eAAe,EAAE,SAAS;QAC1BC,OAAO,EAAE;UACLC,OAAO,EAAE,MAAM;UACfC,IAAI,EAAE,KAAK;UACXC,WAAW,EAAE;YAAE;YACXC,IAAI,EAAE,QAAO,CAAE;UACnB;QACJ,CAAC;;QACDC,MAAM,EAAE;UACJC,YAAY,EAAE,KAAK;UACnBC,SAAS,EAAE;YACPC,KAAK,EAAC;UACV;QACJ,CAAC;QACDC,IAAI,EAAE;UACFC,IAAI,EAAE,IAAI;UACVC,KAAK,EAAE,IAAI;UACXC,MAAM,EAAE,IAAI;UACZC,YAAY,EAAE;QAClB,CAAC;QACDC,KAAK,EAAE;UACHC,IAAI,EAAE;QACV,CAAC;QACDC,KAAK,EAAE,CAAC;UACJC,SAAS,EAAE;YACPf,IAAI,EAAE;UACV,CAAC;UACDE,IAAI,EAAE,OAAO;UACbF,IAAI,EAAE;QAEV,CAAC,CAAC;QACFgB,KAAK,EAAE;UACHd,IAAI,EAAE,UAAU;UAChBlB,IAAI,EAAEA,IAAI,CAACiC,GAAG,CAACC,IAAG,IAAKA,IAAI,CAACC,SAAS,CAAC;UACtCxC,IAAI,EAAE;QACV,CAAC;QACDyC,MAAM,EAAE,CAAC;UACLzC,IAAI,EAAE,OAAO;UACbuB,IAAI,EAAE,KAAK;UACXlB,IAAI,EAAEA,IAAI,CAACiC,GAAG,CAACC,IAAG,IAAKA,IAAI,CAACG,KAAK,CAAC;UAClCC,KAAK,EAAE;YACHtB,IAAI,EAAE,IAAI;YACVuB,QAAQ,EAAE;UACd;QACJ,CAAC;MACL,CAAC;MAED7B,KAAK,CAAC8B,SAAS,CAAC5B,MAAM,CAAC;IAC3B;EACJ